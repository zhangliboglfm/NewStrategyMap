	/*echarts图表轮播*/
	function echartstooltips(){
		clearInterval(timer1);
		clearInterval(timer2);
		clearInterval(timer3);
		//echartbaliliang,echartschannel;
		
		var i=0;
			/*显示tooltips*/
			echartKindCharts.dispatchAction({
				type:'showTip',
				seriesIndex:0,
				dataIndex:i
				
			});
			/*高亮显示图形*/
			echartKindCharts.dispatchAction({
				type:'highlight',
				seriesIndex:0,
				dataIndex:i
			});
		timer1=setInterval(function(){
			if(i>packDisdata[lunboindex].datax.length-1){
				i=0;
			}
			/*显示tooltips*/
			echartKindCharts.dispatchAction({
				type:'showTip',
				seriesIndex:0,
				dataIndex:i
				
			});
			/*高亮显示图形*/
			echartKindCharts.dispatchAction({
				type:'highlight',
				seriesIndex:0,
				dataIndex:i
			});
			i++;
		},2000);
		
		var j=qudaoDistrData[lunboindex].datax.length-1;
			/*显示tooltips*/
			echartschannel.dispatchAction({
				type:'showTip',
				seriesIndex:0,
				dataIndex:j
			});
			/*高亮显示图形*/
			echartschannel.dispatchAction({
				type:'highlight',
				seriesIndex:0,
				dataIndex:j
			});
		timer2=setInterval(function(){
			
			if(j<0){
				j=qudaoDistrData[lunboindex].datax.length-1;
			}
			/*显示tooltips*/
			echartschannel.dispatchAction({
				type:'showTip',
				seriesIndex:0,
				dataIndex:j
			});
			/*高亮显示图形*/
			echartschannel.dispatchAction({
				type:'highlight',
				seriesIndex:0,
				dataIndex:j
			});
			j--;
		},2000);
		
		var k=0;
		/*显示tooltips*/
			echartbaliliang.dispatchAction({
				type:'showTip',
				seriesIndex:0,
				dataIndex:k
			});
			/*高亮显示图形*/
			echartbaliliang.dispatchAction({
				type:'highlight',
				seriesIndex:0,
				dataIndex:k
			});
		timer3=setInterval(function(){
			
			if(k>banliliangData[lunboindex].dataxName.length-1){
				k=0;
			}
			/*显示tooltips*/
			echartbaliliang.dispatchAction({
				type:'showTip',
				seriesIndex:0,
				dataIndex:k
			});
			/*高亮显示图形*/
			echartbaliliang.dispatchAction({
				type:'highlight',
				seriesIndex:0,
				dataIndex:k
			});
			k++;
		},2000);
		var timerdata=new Array();
		timerdata.push(packDisdata[lunboindex].datax.length);
		timerdata.push(qudaoDistrData[lunboindex].datax.length);
		timerdata.push(banliliangData[lunboindex].dataxName.length);
		timerN=timerdata.max();
		/* if(state=="start"){
			clearInterval(timerzong);
			lunbofunc(timerN);
		}; */
	}
	
	
